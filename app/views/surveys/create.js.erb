<% if @survey.save %>
	$('#successful_message').html("Survey title created successfully.");
	$('#survey_form').html("<%= j render partial: 'dashboard/shared/question_form', 
																											locals: {survey_id: @survey.id}%>");
	timeOut();
<% else %>
	$('.error-explanation').html("<%= j render partial: 'dashboard/shared/error_messages', 
							locals: {messages: @survey.errors.full_messages} %>");
<% end %>		

